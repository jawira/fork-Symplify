{
    "name": "symplify/autodiscovery",
    "description": "Forget manual registration of Doctrine entities, Twig templates and routes. Let autodiscovery handle that for you.",
    "license": "MIT",
    "bin": [
        "bin/autodiscovery"
    ],
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.0",
        "symfony/console": "^4.4|^5.1",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symfony/routing": "^4.4|^5.1",
        "symplify/package-builder": "^8.4",
        "symplify/smart-file-system": "^8.4",
        "symplify/autowire-array-parameter": "^8.4"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^1.3.3",
        "doctrine/cache": "^1.10",
        "doctrine/doctrine-cache-bundle": "^1.4",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/twig-bundle": "^4.4|^5.1",
        "symfony/templating": "^4.4|^5.1",
        "symfony/translation": "^4.4|^5.1",
        "symplify/easy-testing": "^8.4",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Autodiscovery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Autodiscovery\\Tests\\": "tests"
        },
        "classmap": [
            "tests/Source/KernelProjectDir"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
