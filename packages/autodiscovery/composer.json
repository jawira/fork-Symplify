{
    "name": "symplify/autodiscovery",
    "description": "Forget manual registration of Doctrine entities, Twig templates and routes. Let autodiscovery handle that for you.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "symfony/dependency-injection": "^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symfony/routing": "^4.4|^5.1",
        "symplify/package-builder": "^9.1",
        "symplify/smart-file-system": "^9.1",
        "symplify/symplify-kernel": "^9.1"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.2",
        "doctrine/common": "^3.1",
        "doctrine/orm": "^2.7",
        "doctrine/persistence": "^2.1",
        "doctrine/cache": "^1.10",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/doctrine-bridge": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/twig-bundle": "^4.4|^5.1",
        "symfony/templating": "^4.4|^5.1",
        "symfony/translation": "^4.4|^5.1",
        "symplify/easy-testing": "^9.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\Autodiscovery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\Autodiscovery\\Tests\\": "tests"
        },
        "classmap": [
            "tests/Source/KernelProjectDir"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
