{
    "name": "symplify/flex-loader",
    "description": "Reduce Kernel Flex boiler code and use extra features",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.0",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/routing": "^4.4|^5.1"
    },
    "require-dev": {
        "symfony/framework-bundle": "^4.4|^5.1",
        "symplify/package-builder": "^8.4",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\FlexLoader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\FlexLoader\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.4-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
