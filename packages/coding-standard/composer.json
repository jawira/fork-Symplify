{
    "name": "symplify/coding-standard",
    "description": "Set of Symplify rules for PHP_CodeSniffer and PHP CS Fixer.",
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "nette/utils": "^3.0",
        "nette/finder": "^2.5",
        "slevomat/coding-standard": "^6.4",
        "friendsofphp/php-cs-fixer": "^2.17.1",
        "symplify/symplify-kernel": "^9.1",
        "symplify/package-builder": "^9.1",
        "symplify/autowire-array-parameter": "^9.1",
        "symplify/rule-doc-generator": "^9.1"
    },
    "require-dev": {
        "nette/application": "^3.0.7",
        "nette/bootstrap": "^3.0",
        "symplify/easy-coding-standard-tester": "^9.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "doctrine/orm": "^2.7",
        "symplify/smart-file-system": "^9.1",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symplify/php-config-printer": "^9.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Symplify\\CodingStandard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplify\\CodingStandard\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
